int inicializarEmpleado(eEmpleado* array[], int tam) {
	int retorno = -1;
	if (array !=NULL && tam > 0) {
		for (int i = 0; i < tam; i++) {
			array[i].isEmpty=1;
			array[i].id =0;
			strcpy(array[i].nombre, " ");
			strcpy(array[i].apellido, " ");
			array[i].sector=' ';
			array[i].sueldo=0;
		}
		retorno = 0;
	}
	return retorno;
}
