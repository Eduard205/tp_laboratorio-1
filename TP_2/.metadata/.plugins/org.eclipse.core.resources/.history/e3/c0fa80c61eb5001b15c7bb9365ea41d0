/*
 * ArrayEmpleados.c
 *Desarrollo de funciones de la biblioteca ArrayEmpleados.h
 */
#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>
#include <string.h>
#include "ArrayEmpleados.h"

int inicializarEmpleado(eEmpleado* array[], int tam) {
	int devolucion= -1;
	if (array != NULL && tam > 0) {
		for (int i = 0; i < tam; i++) {
			*array[i].isEmpty=1;
			array[i].id =0;
			strcpy(array[i].nombre, " ");
			strcpy(array[i].apellido, " ");
			array[i].sector=' ';
			array[i].sueldo=0;
		}
		devolucion= 0;
	}
	return devolucion;
}
